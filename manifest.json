{
  "manifest_version": 2,
  "name": "Youtube 2 NSFW",
  "description": "This extension allows you to watch NSFW youtube video without login",
  "version": "1.0",
  "icons": {
    "128": "images/icon128.png",
    "16": "images/icon16.png",
    "48": "images/icon48.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "main.html"
  },
  "permissions": [
    "activeTab"
  ]
}